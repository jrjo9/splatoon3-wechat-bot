<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mayday9.splatoonbot.business.infrastructure.mapper.TSysFileMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap"
               type="com.mayday9.splatoonbot.business.entity.TSysFile">
                <result column="id" property="id"/>
                <result column="is_delete" property="isDelete"/>
                <result column="create_by" property="createBy"/>
                <result column="update_by" property="updateBy"/>
                <result column="create_time" property="createTime"/>
                <result column="update_time" property="updateTime"/>
                    <result column="biz_id" property="bizId"/>
                    <result column="biz_args" property="bizArgs"/>
                    <result column="file_title" property="fileTitle"/>
                    <result column="file_type" property="fileType"/>
                    <result column="sort" property="sort"/>
                    <result column="file_url" property="fileUrl"/>
                    <result column="file_size" property="fileSize"/>
                    <result column="width" property="width"/>
                    <result column="height" property="height"/>
        </resultMap>

            <!-- 通用查询结果列 -->
            <sql id="Base_Column_List">
                    id,
                    is_delete,
                    create_by,
                    update_by,
                    create_time,
                    update_time,
                biz_id, biz_args, file_title, file_type, sort, file_url, file_size, width, height
            </sql>

</mapper>
